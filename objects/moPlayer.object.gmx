            <kind>1</kind>
            <string>/// Inputs

rightkey = keyboard_check(ord("D"));
leftkey = -keyboard_check(ord("A"));
jumpkey = keyboard_check_pressed(vk_space);

move = leftkey + rightkey;
hspd = move * movespeed;
if (vspd &lt; 10) vspd += grav;

if place_meeting (x,y+1,oBlock)
{
    jumps = jumpsmax
} 

if (jumpkey) &amp;&amp; (jumps &gt; 0)
{
    jumps -= 1;
    vspd = jumpkey * -jumpspeed;
}

// H collision
if (place_meeting(x+hspd,y,oBlock)) {
    while(!place_meeting(x+sign(hspd),y,oBlock)) {
        x += sign(hspd);
    }
    hspd = 0;
}
x += hspd;

// V collsion
if (place_meeting(x,y+vspd,oBlock)) {
    while(!place_meeting(x,y+sign(vspd),oBlock)) {
        y += sign(vspd);
    }
    vspd = 0;
}
y += vspd;

//Animate
if (move!=0) image_xscale = move  * global.orng;
</string>
